    "Check_Visit_Type": {
      "type": "Simple",
      "distributed_transition": [
        {
          "distribution": 0.46,
          "transition": "Therapy_Visit"
        },
        {
          "distribution": 0.54,
          "transition": "Nursing_Visit"
        }
      ]
    },
    "Therapy_Visit": {
